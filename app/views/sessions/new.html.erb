<h1> Login</h1>  

<%= flash[:error] %>
<br>
<p><%= form_for(:user) do |f|%>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Login" %>
</p>

<br>OR<br>
<br>

<%= link_to('Log in with Facebook!', '/auth/facebook') %>
<%# <%= link_to 'Sign in with Spotify', '/auth/spotify' %> 

<%end%>